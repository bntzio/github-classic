{
  "manifest_version": 2,
  "name": "GitHub Classic",
  "version": "1.1.0",
  "description": "Chrome extension to return to the classic GitHub look",
  "minimum_chrome_version": "26",
  "icons": {
    "16": "assets/icon_16.png",
    "48": "assets/icon_48.png",
    "128": "assets/icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://github.com/*",
        "*://*.github.com/*"
      ],
      "css": [
        "src/styles.css"
      ],
      "js": [
        "src/content.js"
      ],
      "run_at": "document_start"
    }
  ]
}
